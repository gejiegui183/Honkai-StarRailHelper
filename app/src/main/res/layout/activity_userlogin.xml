<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/repeat_bg"
        tools:context=".ui.home.function.UserloginActivity" tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp">
    <ImageView
            android:layout_width="358dp"
            android:layout_height="166dp" app:srcCompat="@drawable/pic01" android:id="@+id/imageView35"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.457"
            app:layout_constraintVertical_bias="0.046"/>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="267dp"
            android:layout_height="78dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.304">
        <EditText
                android:id="@+id/et_username"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_marginTop="30dp"
                android:paddingStart="95dp"
                android:paddingEnd="0dp"
                android:background="@drawable/pic11_fin"
                android:inputType="number"
                android:maxLength="11"
                android:textColor="@color/black"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="110dp"
            android:layout_height="45dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.47">
        <ImageButton
                android:layout_width="match_parent"
                android:layout_height="wrap_content" app:srcCompat="@drawable/picbtn" android:id="@+id/imageButton20"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>